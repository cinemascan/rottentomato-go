{
    "swagger": "2.0",
    "info": {
        "contact": {}
    },
    "paths": {
        "/movie": {
            "get": {
                "description": "scrapes https://rottentomatoes.com/search url and returns the top result that matches title, year params provided",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "movie"
                ],
                "summary": "top search result scraped from rotten tomato",
                "parameters": [
                    {
                        "minLength": 1,
                        "type": "string",
                        "description": "movie title",
                        "name": "title",
                        "in": "query"
                    },
                    {
                        "maximum": 2023,
                        "minimum": 1972,
                        "type": "integer",
                        "description": "movie release year",
                        "name": "year",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "title year",
                        "schema": {
                            "$ref": "#/definitions/rotten_tomato.RTMovieInfo"
                        }
                    }
                }
            }
        },
        "/ping": {
            "get": {
                "description": "do ping",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "ping"
                ],
                "summary": "ping example",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "rotten_tomato.RTMovieInfo": {
            "type": "object",
            "properties": {
                "audienceScore": {
                    "$ref": "#/definitions/rotten_tomato.RTScore"
                },
                "genres": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "rating": {
                    "type": "string"
                },
                "runtime": {
                    "type": "string"
                },
                "title": {
                    "type": "string"
                },
                "tomatometerScore": {
                    "$ref": "#/definitions/rotten_tomato.RTScore"
                },
                "year": {
                    "type": "integer"
                }
            }
        },
        "rotten_tomato.RTScore": {
            "type": "object",
            "properties": {
                "averageRating": {
                    "type": "string"
                },
                "bandedRatingCount": {
                    "type": "string"
                },
                "likedCount": {
                    "type": "integer"
                },
                "notLikedCount": {
                    "type": "integer"
                },
                "ratingCount": {
                    "type": "integer"
                },
                "reviewCount": {
                    "type": "integer"
                },
                "state": {
                    "type": "string"
                },
                "value": {
                    "type": "integer"
                }
            }
        }
    }
}